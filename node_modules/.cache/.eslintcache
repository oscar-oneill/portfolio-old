rtfolio/src/Components/About.js",["126"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Login.js",["127"],"import React, { useState, useContext } from 'react';\nimport '../styles/Login.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\nimport { AppContext } from '../Context/AppContext'\n\nconst Login = () => {\n    const [username, setUsername] = useState(\"\")\n    const [password, setPassword] = useState(\"\")\n    const { setIsAuthenticated } = useContext(AppContext)\n\n    const handleSubmit = (e) => {\n        e.preventDefault()\n\n        try {\n            BlogConnection.post('/auth/login', {\n                username, password\n            }).then(res => {\n                console.table(res)\n                localStorage.setItem(\"token\", res.data.data.token)\n                setUsername(\"\")\n                setPassword(\"\")\n                setIsAuthenticated(true)\n                window.location.href = '/blog'\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    return (\n        <form className=\"access_container\" onSubmit={handleSubmit}>\n            <input value={username} onChange={e => setUsername(e.target.value)} name=\"Username\" type=\"text\" placeholder=\"Username\" required></input>\n            <input value={password} onChange={e => setPassword(e.target.value)} name=\"Password\" type=\"password\" placeholder=\"Password\" required></input>\n            <button className=\"submit-form\" type=\"submit\">Submit</button>\n        </form>\n    )\n}\n\nexport default Login","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Update.js",["128","129"],"import React, { useState, useEffect } from 'react'\nimport UpdateForm from '../Components/UpdateForm'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Update = () => {\n    let id = window.location.pathname.slice(11)\n    const [data, setData] = useState(null)\n    const [isLoaded, setIsLoaded] = useState(false)\n    let token = localStorage.getItem('token')\n\n    document.title = 'Editing A Blog Post';\n\n    useEffect(() => {\n        getBlog()\n    }, [])\n\n    const getBlog = () => {\n        BlogConnection.get(`/posts/${id}`)\n        .then(res => {\n            setData(res.data)\n            setIsLoaded(true)\n        })\n    }\n\n    if (!token) {\n        window.location.href = '/blog'\n    } else {\n        if (!isLoaded) {\n            return 'Loading...'\n        } else {\n            return (\n                <div className=\"submit_container\">\n                    <h2 className=\"submit_header\">Edit A Blog Post</h2>\n                    <UpdateForm\n                        id={id}\n                        newTitle={data.post_title}\n                        newBlogger={data.blogger}\n                        newText={data.post_text}\n                        newImage={data.background_image}\n                        newDescription={data.description}\n                    />\n                </div>\n            )\n        }\n    }\n}\n\nexport default Update\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Submit.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Register.js",["130"],"import React, { useState } from 'react';\nimport '../styles/Login.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Register = () => {\n    const [first, setFirst] = useState(\"\")\n    const [last, setLast] = useState(\"\")\n    const [username, setUsername] = useState(\"\")\n    const [password, setPassword] = useState(\"\")\n\n    let token = localStorage.getItem('token')\n\n    const handleSubmit = (e) => {\n        e.preventDefault()\n\n        let first_name = first;\n        let last_name = last;\n\n        try {\n            BlogConnection.post('/auth/register', {\n                first_name, last_name, username, password\n            }).then(res => {\n                console.log(res)\n                setFirst(\"\")\n                setLast(\"\")\n                setUsername(\"\")\n                setPassword(\"\")\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    if (!token) {\n        window.location.href = '/blog'\n    } else {\n        return (\n            <form className=\"access_container\" onSubmit={handleSubmit}>\n                <input value={first} placeholder=\"First Name\" onChange={e => setFirst(e.target.value)} type=\"text\" required/>\n                <input value={last} placeholder=\"Last Name\" onChange={e => setLast(e.target.value)} type=\"text\" required/>\n                <input value={username} placeholder=\"Username\" onChange={e => setUsername(e.target.value)} type=\"text\" required/>\n                <input value={password} placeholder=\"Password\" onChange={e => setPassword(e.target.value)} type=\"password\" required/>\n                <button className=\"submit-form\" type=\"submit\">Submit</button>\n            </form>\n        )\n    }\n}\n\nexport default Register\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Portfolio.js",["131"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Blogs.js",["132"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/ScrollToTop.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Navigation.js",["133","134"],"import React, { useEffect, useRef, useState, useContext } from 'react'\nimport { AppContext } from '../Context/AppContext'\nimport { Link } from 'react-router-dom'\nimport '../styles/Navigation.css'\nimport res from '../media/documents/oscaroneillResume.pdf'\nimport scrollHeight from '../Hooks/Size'\nimport { navigationScheme } from '../Hooks/NavigationScheme'\n\nconst Navigation = () => {\n    const dropdownRef = useRef(null)\n    const navigationRef = useRef()\n    const [isActive, setIsActive] = useState(false)\n    const onClick = () => setIsActive(!isActive)\n    const { setNavigationData, introNavData } = useContext(AppContext)\n    const size = scrollHeight()\n\n    useEffect(() => {\n        const { scrollTop, scrollHeight, clientHeight, offsetTop } = navigationRef.current;\n\n        setNavigationData({\n            clientHeight: clientHeight,\n            scrollTop: scrollTop,\n            scrollHeight: scrollHeight,\n            offsetTop: offsetTop\n        })\n\n    }, [navigationRef])\n\n    const scrollEvent = () => {\n        if (size.scrollY < (introNavData.offsetTop + introNavData.clientHeight)) {\n            return 0\n\n        } else if (size.scrollY > (introNavData.offsetTop + introNavData.clientHeight)) {\n            return 1\n\n        } \n    }\n\n    useEffect(() => {\n        scrollEvent()\n    }, [size])\n\n    const scrollLocation = scrollEvent();\n\n    const navigationStyle = () => {\n        if (scrollLocation === 0) {\n            return navigationScheme.default\n\n        } else if (scrollLocation === 1) {\n            return navigationScheme.intro\n\n        }\n    }\n\n    return (\n        <div \n            style={navigationStyle()} \n            className=\"nav__container\" \n            ref={navigationRef}\n        >\n            <div className=\"nav__home nav\">\n                <span><a href=\"/\">Oscar O'Neill</a></span>\n            </div>\n\n            <div onClick={() => {onClick();}} className=\"mobile__menu\">&#9776;</div>\n            <div ref={dropdownRef} className={`blanket ${ isActive ? 'active' : 'inactive' }`}></div>\n\n            <div ref={dropdownRef} className={`nav__items ${ isActive ? 'active' : 'inactive' }`}>\n                <div onClick={onClick} className=\"nav__links nav\">\n                    <a href=\"#about\">About Me</a>\n                </div>\n\n                <div onClick={onClick} className=\"nav__links nav\">\n                    <a href=\"#portfolio\">Portfolio</a>\n                </div>\n\n                <div onClick={onClick} className=\"nav__links nav\">\n                    <a href={res}>Resume</a>\n                </div>\n\n                <div onClick={onClick} className=\"nav__links nav\">\n                    <Link to=\"/blog\">Blog</Link>\n                </div>\n\n                <div onClick={onClick} className=\"nav__links nav\">\n                    <a href=\"mailto:oscaroneill@email.com\">Contact</a>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Navigation\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/SinglePost.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Projects.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/BlogNavigation.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Form.js",["135"],"import React, { useState } from 'react'\nimport '../styles/Form.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Form = () => {\n    const [title, setTitle] = useState(\"\");\n    const [blogger, setBlogger] = useState(\"\");\n    const [text, setText] = useState(\"\");\n    const [bgImage, setBgImage] = useState(\"\");\n    const [description, setDescription] = useState(\"\");\n\n    const [countTitle, setCountTitle] = useState(0);\n    const [countDescription, setCountDescription] = useState(0);\n\n\n    const calculateTitle = (e) => {\n        setCountTitle(e.target.value.length)\n    }\n\n    const calculateDescription = (e) => {\n        setCountDescription(e.target.value.length)\n    }\n\n    const submitPost = async (e) => {\n        e.preventDefault();\n        const post_title = title;\n        const post_text = text;\n        const background_image = bgImage\n\n        try {   \n            BlogConnection.post('/posts/add', {\n                post_title, post_text, blogger, background_image, description\n            })\n            .then(res => {\n                window.location.href = '/blog'\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    return (\n        <form className=\"form_container\" onSubmit={submitPost}>\n            <div className=\"form_input\">\n                <label>Post Title: {countTitle}/150</label>\n\t\t\t\t<input value={title} onChange={(e) => {setTitle(e.target.value); calculateTitle(e)}} name=\"title\" type=\"text\" placeholder=\"Post Title\" required/>\n            </div>\n\n            <div className=\"form_input\">\n\t\t\t\t<input value={blogger} onChange={(e) => setBlogger(e.target.value)} name=\"blogger\" type=\"text\" placeholder=\"Author\" required/>\n\t\t\t</div>\n\n            <div className=\"form_input\">\n\t\t\t\t<input value={bgImage} onChange={(e) => setBgImage(e.target.value)} name=\"image\" type=\"text\" placeholder=\"Background Image\" required/>\n\t\t\t</div>\n\n            <div className=\"form_input\">\n                <label>Description: {countDescription}/200</label>\n\t\t\t\t<textarea id=\"description\" value={description} onChange={(e) => {setDescription(e.target.value); calculateDescription(e)}} name=\"description\" type=\"text\" placeholder=\"Description\" required/>\n            </div>\n\n            <div className=\"form_input\">\n\t\t\t\t<textarea value={text} onChange={(e) => setText(e.target.value)} name=\"Blog Text\" type=\"text\" placeholder=\"Insert Text Here\" required/>\n\t\t\t</div>\n\n            <div className=\"form_button\">\n                <button className=\"form_submit\">Submit</button>\n            </div>\n        </form>\n    )\n}\n\nexport default Form","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/UpdateForm.js",["136"],"import React, {useState} from 'react'\nimport '../styles/Form.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Form = ({id, newTitle, newBlogger, newText, newImage, newDescription}) => {\n    const [title, setTitle] = useState(newTitle);\n    const [blogger, setBlogger] = useState(newBlogger);\n    const [text, setText] = useState(newText);\n    const [bgImage, setBgImage] = useState(newImage);\n    const [description, setDescription] = useState(newDescription);\n\n    const [countTitle, setCountTitle] = useState(0);\n    const [countDescription, setCountDescription] = useState(0);\n\n    const calculateTitle = (e) => {\n        setCountTitle(e.target.value.length)\n    }\n\n    const calculateDescription = (e) => {\n        setCountDescription(e.target.value.length)\n    }\n\n    const submitPost = async (e) => {\n        e.preventDefault();\n        const post_title = title;\n        const post_text = text;\n        const background_image = bgImage\n\n        try {   \n            BlogConnection.post(`/posts/update/${id}`, {\n                id, post_title, post_text, blogger, background_image, description\n            })\n            .then(res => {\n                window.location.href = `/blog/post/${id}`\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    return (\n        <form className=\"form_container\" onSubmit={submitPost}>\n            <div className=\"form_input\">\n                <label>Post Title: {countTitle}/150</label>\n\t\t\t\t<input value={title} onChange={(e) => {setTitle(e.target.value); calculateTitle(e)}} name=\"title\" type=\"text\" placeholder=\"Post Title\" required/>\n            </div>\n\n            <div className=\"form_input\">\n\t\t\t\t<input value={blogger} onChange={(e) => setBlogger(e.target.value)} name=\"blogger\" type=\"text\" placeholder=\"Author\" required/>\n\t\t\t</div>\n\n            <div className=\"form_input\">\n\t\t\t\t<input value={bgImage} onChange={(e) => setBgImage(e.target.value)} name=\"image\" type=\"text\" placeholder=\"Background Image\" required/>\n\t\t\t</div>\n\n            <div className=\"form_input\">\n                <label>Description: {countDescription}/200</label>\n\t\t\t\t<textarea id=\"description\" value={description} onChange={(e) => {setDescription(e.target.value); calculateDescription(e)}} name=\"description\" type=\"text\" placeholder=\"Description\" required/>\n            </div>\n\n            <div className=\"form_input\">\n\t\t\t\t<textarea value={text} onChange={(e) => setText(e.target.value)} name=\"Blog Text\" type=\"text\" placeholder=\"Insert Text Here\" required/>\n\t\t\t</div>\n\n            <div className=\"form_button\">\n                <button className=\"form_submit\">Update</button>\n            </div>\n        </form>\n    )\n}\n\nexport default Form","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Posts.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/API/BlogConnection.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/AuthItems.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Items.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Intro.js",["137"],"import React, { useEffect, useContext, useRef } from 'react'\nimport { Link } from 'react-router-dom'\nimport { AppContext } from '../Context/AppContext'\nimport '../styles/Intro.css';\nimport resume from '../media/documents/oscaroneillResume.pdf';\n\nconst Intro = () => {\n    const introNavInnerRef = useRef()\n    const { setIntroNavData } = useContext(AppContext)\n\n    useEffect(() => {\n        const { scrollTop, scrollHeight, clientHeight, offsetTop } = introNavInnerRef.current;\n\n        setIntroNavData({\n            clientHeight: clientHeight,\n            scrollTop: scrollTop,\n            scrollHeight: scrollHeight,\n          