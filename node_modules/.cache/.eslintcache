logPage\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/About.js",["239"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Login.js",["240"],"import React, { useState, useContext } from 'react';\nimport '../styles/Login.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\nimport { AppContext } from '../Context/AppContext'\n\nconst Login = () => {\n    const [username, setUsername] = useState(\"\")\n    const [password, setPassword] = useState(\"\")\n    const { setIsAuthenticated } = useContext(AppContext)\n\n    const handleSubmit = (e) => {\n        e.preventDefault()\n\n        try {\n            BlogConnection.post('/auth/login', {\n                username, password\n            }).then(res => {\n                console.table(res)\n                localStorage.setItem(\"token\", res.data.data.token)\n                setUsername(\"\")\n                setPassword(\"\")\n                setIsAuthenticated(true)\n                window.location.href = '/blog'\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    return (\n        <form className=\"access_container\" onSubmit={handleSubmit}>\n            <input value={username} onChange={e => setUsername(e.target.value)} name=\"Username\" type=\"text\" placeholder=\"Username\" required></input>\n            <input value={password} onChange={e => setPassword(e.target.value)} name=\"Password\" type=\"password\" placeholder=\"Password\" required></input>\n            <button className=\"submit-form\" type=\"submit\">Submit</button>\n        </form>\n    )\n}\n\nexport default Login","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Update.js",["241","242"],"import React, { useState, useEffect } from 'react'\nimport UpdateForm from '../Components/UpdateForm'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Update = () => {\n    let id = window.location.pathname.slice(11)\n    const [data, setData] = useState(null)\n    const [isLoaded, setIsLoaded] = useState(false)\n    let token = localStorage.getItem('token')\n\n    document.title = 'Editing A Blog Post';\n\n    useEffect(() => {\n        getBlog()\n    }, [])\n\n    const getBlog = () => {\n        BlogConnection.get(`/posts/${id}`)\n        .then(res => {\n            setData(res.data)\n            setIsLoaded(true)\n        })\n    }\n\n    if (!token) {\n        window.location.href = '/blog'\n    } else {\n        if (!isLoaded) {\n            return 'Loading...'\n        } else {\n            return (\n                <div className=\"submit_container\">\n                    <h2 className=\"submit_header\">Edit A Blog Post</h2>\n                    <UpdateForm\n                        id={id}\n                        newTitle={data.post_title}\n                        newBlogger={data.blogger}\n                        newText={data.post_text}\n                        newImage={data.background_image}\n                        newDescription={data.description}\n                    />\n                </div>\n            )\n        }\n    }\n}\n\nexport default Update\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Submit.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Register.js",["243"],"import React, { useState } from 'react';\nimport '../styles/Login.css'\nimport axios from 'axios'\nimport BlogConnection from '../API/BlogConnection'\n\nconst Register = () => {\n    const [first, setFirst] = useState(\"\")\n    const [last, setLast] = useState(\"\")\n    const [username, setUsername] = useState(\"\")\n    const [password, setPassword] = useState(\"\")\n\n    let token = localStorage.getItem('token')\n\n    const handleSubmit = (e) => {\n        e.preventDefault()\n\n        let first_name = first;\n        let last_name = last;\n\n        try {\n            BlogConnection.post('/auth/register', {\n                first_name, last_name, username, password\n            }).then(res => {\n                console.log(res)\n                setFirst(\"\")\n                setLast(\"\")\n                setUsername(\"\")\n                setPassword(\"\")\n            })\n        } catch (err) {\n            console.error(\"Error: \", err.message);\n        }\n    }\n\n    if (!token) {\n        window.location.href = '/blog'\n    } else {\n        return (\n            <form className=\"access_container\" onSubmit={handleSubmit}>\n                <input value={first} placeholder=\"First Name\" onChange={e => setFirst(e.target.value)} type=\"text\" required/>\n                <input value={last} placeholder=\"Last Name\" onChange={e => setLast(e.target.value)} type=\"text\" required/>\n                <input value={username} placeholder=\"Username\" onChange={e => setUsername(e.target.value)} type=\"text\" required/>\n                <input value={password} placeholder=\"Password\" onChange={e => setPassword(e.target.value)} type=\"password\" required/>\n                <button className=\"submit-form\" type=\"submit\">Submit</button>\n            </form>\n        )\n    }\n}\n\nexport default Register\n","/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Portfolio.js",["244"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/routes/Blogs.js",["245"],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/ScrollToTop.js",[],"/Users/oscaroneill/Documents/Visual Studio Code/react/portfolio/src/Components/Navigation.js",["246","247"],"import React, { useEffect, useRef, useState, useContext } from 'react'\nimport { AppContext } from '../Context/AppContext'\nimport { Link } from 'react-router-dom'\nimport '../styles/Navigation.css'\nimport res from '../media/documents/oscaroneillResume.pdf'\nimport scrollHeight from '../Hooks/Size'\nimpor